{
    "index_patterns": [
      "log4j2-elasticsearch-hugegraph-log-*"
    ],
    "settings": {
      "index": {
        "refresh_interval": "5s"
      }
    },
    "mappings": {
      "properties": {
        "timeMillis": {
          "type": "date",
          "format": "epoch_millis"
        }
      }
    }
  }
  